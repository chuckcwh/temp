<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  <%_ urls.forEach(url => { _%>
  <url>
    <loc><%= config.url %><%= url.loc %></loc>
  </url>
  <%_ }); _%>
  <%_ subCategories.forEach(subCategory => { _%>
  <url>
    <loc><%= config.url %>/services/<%= subCategory.slug %></loc>
  </url>
  <%_ }); _%>
</urlset>
